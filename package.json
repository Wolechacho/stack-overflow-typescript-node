{
  "name": "korapay-app",
  "version": "1.0.0",
  "description": "A simple clone of stackoverflow app",
  "main": "server.js",
  "scripts": {
    "build": "gulp build",
    "test": "jest --detectOpenHandles",
    "start": "tsc && node dist/server.js",
    "dev:server": "nodemon --watch './**/*.ts' --exec 'ts-node' server.ts",
    "sequelize:create": "./node_modules/.bin/sequelize db:create",
    "sequelize:migrate-up": "./node_modules/.bin/sequelize db:migrate",
    "sequelize:migrate-down": "./node_modules/.bin/sequelize db:migrate:undo",
    "sequelize:seed-up": "./node_modules/.bin/sequelize db:seed:all",
    "sequelize:seed-undo": "./node_modules/.bin/sequelize db:seed:undo:all"
  },
  "keywords": [
    "stackoverflow",
    "typescript",
    "node.js",
    "mysql",
    "javascript"
  ],
  "author": "Wole Adenigbagbe",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/config": "0.0.38",
    "@types/express": "^4.17.11",
    "@types/express-validator": "^3.0.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mysql": "^2.15.18",
    "@types/node": "^15.0.1",
    "@types/nodemailer": "^6.4.1",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.1.3",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-tslint": "^8.1.4",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "config": "^3.3.6",
    "express": "^4.17.1",
    "express-validator": "^6.10.1",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.6.0",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.2",
    "sequelize-typescript": "^2.1.0",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  }
}
